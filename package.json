{
   "name": "rentread",
   "private": true,
   "version": "1.0.0",
   "type": "module",
   "scripts": {
      "dev": "vite --config ./configs/build/vite.config.ts --port 3000 --host",
      "build": "tsc && vite build --config ./configs/build/vite.config.ts",
      "preview": "vite preview --config ./configs/build/vite.config.ts",
      "lint": "biome check ./src",
      "lint:fix": "biome check --fix --unsafe ./src",
      "format": "biome format ./src --write",
      "prepare": "husky",
      "generate-routes": "tsr generate"
   },
   "dependencies": {
      "@tanstack/react-router": "^1.97.14",
      "antd": "^5.23.2",
      "dayjs": "^1.11.13",
      "faker": "^5.5.3",
      "i18next": "^24.2.2",
      "i18next-browser-languagedetector": "^8.0.4",
      "moment": "^2.30.1",
      "pigeon-maps": "^0.22.1",
      "query-string": "^9.1.1",
      "react": "^18.3.1",
      "react-dom": "^18.3.1",
      "react-i18next": "^15.4.1",
      "uuid": "^11.0.5",
      "zustand": "^5.0.3"
   },
   "engines": {
      "node": ">=22.0.0 <23.0.0"
   },
   "devDependencies": {
      "@biomejs/biome": "1.9.4",
      "@tanstack/router-devtools": "^1.97.14",
      "@tanstack/router-plugin": "^1.97.14",
      "@types/faker": "^5.5.9",
