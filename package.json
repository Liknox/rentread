      "@typescript-eslint/parser": "^7.15.0",
      "@vitejs/plugin-react": "^4.3.1",
      "autoprefixer": "^10.4.20",
      "eslint": "^8.57.0",
      "eslint-plugin-react-hooks": "^4.6.2",
      "eslint-plugin-react-refresh": "^0.4.7",
      "husky": "^9.1.7",
      "postcss": "^8.5.1",
      "sass-embedded": "^1.83.4",
      "tailwindcss": "^3.4.17",
      "typescript": "^5.2.2",
      "vite": "^5.3.4",
      "vite-tsconfig-paths": "^5.1.4"
   }
