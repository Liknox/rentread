{
	"name": "rentread",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --open --config ./configs/build/vite.config.ts --port 3000 --host",
		"build": "tsc && vite build --config ./configs/build/vite.config.ts",
		"preview": "vite preview --config ./configs/build/vite.config.ts",
		"lint": "biome check ./src",
		"lint:apply": "biome check --fix --unsafe ./src",
		"format": "biome format ./src --write",
		"prepare": "husky",
		"generate-routes": "tsr generate"
	},
	"dependencies": {
		"@tanstack/react-router": "^1.97.14",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"engines": {
		"node": ">=22.0.0 <23.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@tanstack/router-devtools": "^1.97.14",
		"@tanstack/router-plugin": "^1.97.14",
		"@types/node": "^22.10.7",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.15.0",
		"@typescript-eslint/parser": "^7.15.0",
		"@vitejs/plugin-react": "^4.3.1",
		"autoprefixer": "^10.4.20",
		"eslint": "^8.57.0",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.7",
		"husky": "^9.1.7",
		"postcss": "^8.5.1",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.2.2",
		"vite": "^5.3.4",
		"vite-tsconfig-paths": "^5.1.4"
	}
}
